{
  "expo": {
    "slug": "guideitor",
    "version": "1.0.0",
    "orientation": "portrait",
    "newArchEnabled": true,
    "icon": "./assets/icons/icon-1024.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/splash-2048.png",
      "resizeMode": "contain",
      "backgroundColor": "#003399"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon-foreground.png",
        "backgroundImage": "./assets/icons/adaptive-icon-background.png",
        "backgroundColor": "#003399"
      },
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.joscelino.guideitor"
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    },
    "plugins": [
      "expo-font",
      "react-native-vision-camera",
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8f78e378-ee18-474b-9ae0-9e809d7611d1"
      }
    },
    "owner": "joscelino"
  }
}
